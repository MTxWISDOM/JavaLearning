package algorithm.src.LearnJava.HashMAP;

import java.util.HashMap;
import java.util.Scanner;

public class HashMAP {
    private HashMap<Integer, String> m = new HashMap<>();

    public void setM() {
        m.put(1,"1=1"); // put方法用来增加HashsMap的元素
        m.put(2,"2=2");
        m.put(3,"3=3");
        m.put(4,"4=4");
    }

    public String getValue(int key){
        for( Integer i : m.keySet()){ //遍历出所有的key值，在用get方法利用key值找出相应的值
            String x = m.get(i);
            System.out.println(x);
        }
        if( m.containsKey(key)){ //containKey用来检查是否含有该key
            return m.get(key);
        }
        else{
            return "NOT FOUND";
        }

    }

    public static void main(String args[]){
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        HashMAP h = new HashMAP();
        h.setM();
        String s  = h.getValue(n);
        System.out.println(s);
    }
}
